<p id="notice"><%= notice %></p>

<h1>Interviews</h1>

<table>
  <thead>
    <tr>
      <th>Interviewermail</th>
      <th>Studentmail</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviews.each do |interview| %>
      <tr>
        <td><%= interview.InterviewerMail %></td>
        <td><%= interview.StudentMail %></td>
        <td><%= interview.start_time %></td>
        <td><%= interview.end_time %></td>
        <td><%= link_to 'Show', interview %></td>
        <td><%= link_to 'Update', edit_interview_path(interview) %></td>
        <td><%= link_to 'Delete', interview, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Schedule Interview', new_interview_path %>
